<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--============================ cork ===============================-->
  <ThingDef ParentName="ResourceBase">
    <defName>carnap_cork</defName>
    <label>cork</label>
    <description>Material obtained from the bark of a tree and most commonly used in wine stoppers. It has bubble-form structure and natural fire retardant, making it a good material for insulation and floor tiles. Cork is softer than wood and more susceptible to damage.</description>
    <graphicData>
      <texPath>TMaterials/unique/cork</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Wood_Drop</soundInteract> <!-- Silver_Drop Metal_Drop Wood_Drop ChunkRock_Drop -->
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.2</MarketValue> <!-- silver 1 gold 10 steel 1.9 plasteel 9 wood 1.2 uranium 6 jade 5 stone 0.9 -->
      <Mass>0.2</Mass> <!-- silver 0.008 gold 0.008 steel 0.5 plasteel 0.25 wood 0.4 uranium 1 jade 0.5; 1 = ~16000kg/m3 -->
      <Flammability>0.4</Flammability>
      <MaxHitPoints>60</MaxHitPoints>
      <StuffPower_Armor_Sharp>0.44</StuffPower_Armor_Sharp> <!-- silver 0.72 gold 0.72 steel 0.9  plasteel 1.14 wood 0.54 uranium 1.08 jade 0.9 -->
      <StuffPower_Armor_Blunt>0.51</StuffPower_Armor_Blunt> <!-- silver 0.36 gold 0.36 steel 0.45 plasteel 0.55 wood 0.54 uranium 0.54 jade 0.45 -->
      <StuffPower_Armor_Heat>0.5</StuffPower_Armor_Heat> <!-- silver 0.36 gold 0.36 steel 0.60 plasteel 0.65 wood 0.4  uranium 0.65 jade 0.54 -->
      <StuffPower_Insulation_Cold>9</StuffPower_Insulation_Cold> <!-- silver 0.3  gold 3    steel 3    plasteel 3    wood 8    uranium 3    jade 3 -->
      <StuffPower_Insulation_Heat>5</StuffPower_Insulation_Heat> <!-- silver 0.0  gold 0    steel 0    plasteel 0    wood 4    uranium 0    jade 0 -->
      <SharpDamageMultiplier>0.36</SharpDamageMultiplier> <!-- silver 0.85 gold 0.75 steel 1    plasteel 1.1  wood 0.4  uranium 1.1  jade - stone 0.6 -->
      <BluntDamageMultiplier>0.8</BluntDamageMultiplier> <!-- silver 1.0  gold 1.0  steel 1    plasteel 0.9  wood 0.9  uranium 1.5  jade 1.5 stone 1.0 -->
      <!-- <ConstructionSpeedFactor>value</ConstructionSpeedFactor> -->
      <!-- stone 0.8-->
    </statBases>
    <useHitPoints>true</useHitPoints> <!-- should raw resource item have hitpoints -->
    <healthAffectsPrice>false</healthAffectsPrice>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <burnableByRecipe>true</burnableByRecipe>
    <smeltable>false</smeltable> <!-- idk maybe if can smelt? -->
    <stuffProps>
      <categories>
        <li>Woody</li> <!-- Metallic Woody Stony Fabric -->
      </categories>
      <commonality>0.05</commonality> <!-- silver 0.05 gold 0.02 steel 1.00 plasteel 0.05 wood 1.0 uranium 0.05 jade 0.05 -->
      <constructEffect>ConstructWood</constructEffect> <!-- ConstructMetal ConstructWood -->
      <color>(210,177,114)</color> <!-- R,G,B -->
      <soundImpactStuff>BulletImpact_Wood</soundImpactStuff> <!-- BulletImpact_Metal BulletImpact_Wood -->
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp> <!-- MeleeHit_Metal_Sharp MeleeHit_Wood MeleeHit_Stone -->
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt> <!-- MeleeHit_Metal_Blunt MeleeHit_Wood MeleeHit_Stone -->
      <statOffsets>
        <!---->
      </statOffsets>
      <statFactors>
        <!---->
        <MaxHitPoints>0.55</MaxHitPoints> <!-- silver 0.7 gold 0.6 steel 1 plasteel 2.8 wiid 0.65 uranium 2.5 jade 0.5 stone 1.8 -->
        <Beauty>1</Beauty> <!-- silver 2 gold 2.5 steel 1 plasteel 1 wood 1 uranium 0.5 jade 2.5 -->
        <WorkToMake>0.6</WorkToMake> <!-- gold 0.9 plasteel 2.2 wood 0.7 uranium 1.9 stone 1.3 -->
        <WorkToBuild>0.6</WorkToBuild> <!-- = worktomake, stone 6.0 -->
        <Flammability>0.3</Flammability> <!-- silver 0.4 gold 0.4 steel 0.4 plasteel 0 wood 1 uranium 0 -->
      </statFactors>
    </stuffProps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded> <!-- Light Medium Heavy -->
  </ThingDef>

